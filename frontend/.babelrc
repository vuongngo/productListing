{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime"],
    ["add-module-exports"],
    ["transform-decorators-legacy"],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "app": "./src/app",
        "underscore": "lodash"
      }
    }]
  ]
}
